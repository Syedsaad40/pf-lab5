#include<stdio.h>
int main(){
	int age,check;
	printf("Enter the age:");
	scanf("%d",&age);
	printf("Are you a citizen of pakistan?(enter 1 for yes and 2 for no):");
	scanf("%d", &check );
	if(age>=18 && check==1){
		printf("You are eligible to vote");
	}
	else{
		printf("You are not eligible to vote");
	}
	
	
	
	
	return 0;
}
